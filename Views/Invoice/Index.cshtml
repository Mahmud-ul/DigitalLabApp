@model IEnumerable<Invoice> 
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-md-12">
        <h1 class="text-center">Invoice List</h1><hr/><br />   

        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>InvoiceId</th>
                </tr>
            </thead>
            <tbody>              
                @foreach(Invoice item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.InvoiceDetailId</td>
                    </tr>
                }              
            </tbody>
            <tfoot>
                <tr>
                    <th>Id</th>
                    <th>InvoiceId</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>